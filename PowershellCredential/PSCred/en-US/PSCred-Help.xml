	<command:command xmlns:maml="http://schemas.microsoft.com/maml/2004/10" xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10">
		<!--Command-->
		<command:details>
			<command:name>New-AESPasswordFile</command:name>
			<maml:description>
				<maml:para>This creates a AES Password File that can be used in credential objects.</maml:para>
			</maml:description>
			<maml:copyright>
				<maml:para />
			</maml:copyright>
			<command:verb>New</command:verb>
			<command:noun>AESPasswordFile</command:noun>
			<dev:version />
		</command:details>
		<maml:description>
			<maml:para>This file will use the components of an AES credential which is an encrypted key. It will then use that key and pair it with the password file that it generates.</maml:para>
		</maml:description>
		<command:syntax>
			<!--Parameter Sets-->
			<command:syntaxItem>
				<maml:name>New-AESPasswordFile</maml:name>
			</command:syntaxItem>
		</command:syntax>
		<command:parameters>
			<!--All Parameters-->
			<command:parameter required="false" globbing="false" pipelineInput="false" variableLength="false" position="named">
				<maml:name>Password </maml:name>
				<maml:description>
					<maml:para>The content in this parameter will be  in the password file. It will be encrypted using the  key file that is generated by the New-AESKeyFile cmdlet.</maml:para>
				</maml:description>
				<command:parameterValue required="false" variableLength="false" />
				<dev:defaultValue />
			</command:parameter>
			<command:parameter required="false" globbing="false" pipelineInput="false" variableLength="false" position="named">
				<maml:name>Path </maml:name>
				<maml:description>
					<maml:para>This is the  path of the AESPassword file.</maml:para>
				</maml:description>
				<command:parameterValue required="false" variableLength="false" />
				<dev:defaultValue />
			</command:parameter>
			<command:parameter required="false" globbing="false" pipelineInput="false" variableLength="false" position="named">
				<maml:name>Keyfile</maml:name>
				<maml:description>
					<maml:para>This is the file location of the keyfile.</maml:para>
				</maml:description>
				<command:parameterValue required="false" variableLength="false" />
				<dev:defaultValue />
			</command:parameter>
		</command:parameters>
		<command:examples>
			<!--Examples-->
			<command:example>
				<maml:title>EXAMPLE 1</maml:title>
				<maml:introduction>
					<maml:para />
				</maml:introduction>
				<dev:code>C:\PS&gt; New-AESPasswordFile -path c:\temp\AESPassword.txt -keyfile c:\keys\password.key -password (Read-Host "Enter password" -asSecureString)</dev:code>
				<dev:remarks>
					<maml:para>This will create a password using the the AES key that is located in c:\keys\password.key. It will -prompt- for the password and display as a secure string. The password will not be displayed.</maml:para>
				</dev:remarks>
			</command:example>
			<command:example>
				<maml:title>EXAMPLE 2</maml:title>
				<maml:introduction>
					<maml:para />
				</maml:introduction>
				<dev:code>C:\PS&gt; New-AESPasswordFile -path c:\temp\AESPassword.txt -keyfile c:\keys\password.key -password  password </dev:code>
				<dev:remarks>
					<maml:para>This will create a password using the the AES key that is located in c:\keys\password.key. This is just showing an example of how to pass a plain text password to the function. It is NOT recommended.</maml:para>
				</dev:remarks>
			</command:example>
		</command:examples>
	</command:command>
