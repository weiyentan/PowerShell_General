	<command:command xmlns:maml="http://schemas.microsoft.com/maml/2004/10" xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10">
		<!--Command-->
		<command:details>
			<command:name>New-AESKeyFile</command:name>
			<maml:description>
				<maml:para>This cmdlet produces a  Key file  that can be used in AES type credential passwords.</maml:para>
			</maml:description>
			<maml:copyright>
				<maml:para />
			</maml:copyright>
			<command:verb>New</command:verb>
			<command:noun>AESKeyFile</command:noun>
			<dev:version />
		</command:details>
		<maml:description>
			<maml:para>This cmdlet creates a key file that is used with an AES Encrypted file. This file in conjunction with the password file are used together to reconstruct a credential object</maml:para>
		</maml:description>
		<command:syntax>
			<!--Parameter Sets-->
			<command:syntaxItem>
				<maml:name>New-AESKeyFile</maml:name>
			</command:syntaxItem>
		</command:syntax>
		<command:parameters>
			<!--All Parameters-->
			<command:parameter required="false" globbing="false" pipelineInput="false" variableLength="false" position="named">
				<maml:name>Path </maml:name>
				<maml:description>
					<maml:para>This is the  path of the AES key file.</maml:para>
				</maml:description>
				<command:parameterValue required="false" variableLength="false" />
				<dev:defaultValue />
			</command:parameter>
			<command:parameter required="false" globbing="false" pipelineInput="false" variableLength="false" position="named">
				<maml:name>Byte</maml:name>
				<maml:description>
					<maml:para>This is the bytes that is used to create a key. By default it creates a 32 byte value</maml:para>
				</maml:description>
				<command:parameterValue required="false" variableLength="false" />
				<dev:defaultValue>32</dev:defaultValue>
			</command:parameter>
		</command:parameters>
		<command:examples>
			<!--Examples-->
			<command:example>
				<maml:title>EXAMPLE 1</maml:title>
				<maml:introduction>
					<maml:para />
				</maml:introduction>
				<dev:code>C:\PS&gt; New-AESKeyFile -path c:\temp\aes.key </dev:code>
				<dev:remarks>
					<maml:para>This will create a key file in the temp directory.</maml:para>
				</dev:remarks>
			</command:example>
			<command:example>
				<maml:title>EXAMPLE 2</maml:title>
				<maml:introduction>
					<maml:para />
				</maml:introduction>
				<dev:code>C:\PS&gt; New-AESKeyFile -path c:\Keys\aes.key -byte 32 </dev:code>
				<dev:remarks>
					<maml:para>This will create a 32 byte AES key that is exported to c:\keys\ with the file name aes.keys.</maml:para>
				</dev:remarks>
			</command:example>
		</command:examples>
	</command:command>
	<command:command xmlns:maml="http://schemas.microsoft.com/maml/2004/10" xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10">
		<!--Command-->
		<command:details>
			<command:name>New-AESCredentialObject</command:name>
			<maml:description>
				<maml:para>This will create an AES Credential object from a AES key and password file.</maml:para>
			</maml:description>
			<maml:copyright>
				<maml:para />
			</maml:copyright>
			<command:verb>New</command:verb>
			<command:noun>AESCredentialObject</command:noun>
			<dev:version />
		</command:details>
		<maml:description>
			<maml:para>This cmdlet will reproduce a credential object using AES keys and password text files.</maml:para>
		</maml:description>
		<command:syntax>
			<!--Parameter Sets-->
			<command:syntaxItem>
				<maml:name>New-AESCredentialObject</maml:name>
			</command:syntaxItem>
		</command:syntax>
		<command:parameters>
			<!--All Parameters-->
			<command:parameter required="false" globbing="false" pipelineInput="false" variableLength="false" position="named">
				<maml:name>AESKeyPath</maml:name>
				<maml:description>
					<maml:para>This is the file path of the AES key.</maml:para>
				</maml:description>
				<command:parameterValue required="false" variableLength="false">string </command:parameterValue>
				<dev:defaultValue />
			</command:parameter>
			<command:parameter required="false" globbing="false" pipelineInput="false" variableLength="false" position="named">
				<maml:name>Passwordpath</maml:name>
				<maml:description>
					<maml:para>This is the file path  of the password file.</maml:para>
				</maml:description>
				<command:parameterValue required="false" variableLength="false" />
				<dev:defaultValue />
			</command:parameter>
			<command:parameter required="false" globbing="false" pipelineInput="false" variableLength="false" position="named">
				<maml:name>Identity</maml:name>
				<maml:description>
					<maml:para>This is the ID of the credential object.</maml:para>
				</maml:description>
				<command:parameterValue required="false" variableLength="false">string</command:parameterValue>
				<dev:defaultValue />
			</command:parameter>
		</command:parameters>
		<command:examples>
			<!--Examples-->
			<command:example>
				<maml:title>EXAMPLE 1</maml:title>
				<maml:introduction>
					<maml:para />
				</maml:introduction>
				<dev:code>C:\PS&gt; New-AESCredentialObject -aeskeypath c:\aes\aes.key -passwordpath c:\passwords\password.txt -identity johndoe </dev:code>
				<dev:remarks>
					<maml:para>This will create an Credential object using the password and aes key files that we have generated before.</maml:para>
				</dev:remarks>
			</command:example>
		</command:examples>
	</command:command>
